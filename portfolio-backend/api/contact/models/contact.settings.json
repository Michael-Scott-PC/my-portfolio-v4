{
  "connection": "default",
  "collectionName": "contacts",
  "info": {
    "name": "contact"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "contactName": {
      "type": "string",
      "required": true,
      "minLength": 2,
      "maxLength": 50
    },
    "contactEmail": {
      "type": "email",
      "minLength": 5,
      "maxLength": 40,
      "required": true
    },
    "contactPhone": {
      "type": "string",
      "maxLength": 10,
      "minLength": 10
    },
    "inquiry": {
      "type": "richtext",
      "minLength": 10,
      "maxLength": 1000,
      "required": true
    },
    "contactPreference": {
      "type": "string"
    }
  }
}
